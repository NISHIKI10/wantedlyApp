<h1>登録済みのユーザー一覧</h1>
<table class="table">
  <thead>
    <tr>
      <th>id</th>
      <th>名前</th>
      <th>登録日</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
    	<td><%= user.username %></td>
    	<td><%= user.created_at %></td>
      <td><%= link_to "削除", admin_user_path(user.id), method: :delete, data: {confirm: "本当にいいですか?"} %></td>
      </tr>
    <% end %>
  </tbody>
</table>